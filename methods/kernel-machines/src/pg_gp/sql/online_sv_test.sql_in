-- Example usage for regression:
-- \i online_sv.sql_in

select MADLIB_SCHEMA.svm_generate_reg_data('MADLIB_SCHEMA.svm_train_data', 10000, 5);
select * from MADLIB_SCHEMA.svm_regression('MADLIB_SCHEMA.svm_train_data', 'regs', false, 'MADLIB_SCHEMA.svm_dot');
select * from MADLIB_SCHEMA.svm_predict('regs', '{1,2,4,20,10}', 'MADLIB_SCHEMA.svm_dot') > 0;
select * from MADLIB_SCHEMA.svm_predict('regs', '{1,2,4,20,-10}', 'MADLIB_SCHEMA.svm_dot') < 0;

   
-- To learn multiple support vector regression models
select * from MADLIB_SCHEMA.svm_regression('MADLIB_SCHEMA.svm_train_data', 'regp', true, 'MADLIB_SCHEMA.svm_dot');
select pred.prediction > 0 from MADLIB_SCHEMA.svm_predict_combo('regp', '{1,2,4,20,10}', 'MADLIB_SCHEMA.svm_dot') as pred;
select pred.prediction < 0 from MADLIB_SCHEMA.svm_predict_combo('regp', '{1,2,4,20,-10}', 'MADLIB_SCHEMA.svm_dot') as pred;

create table MADLIB_SCHEMA.svm_reg_test ( id int, ind float8[] );
insert into MADLIB_SCHEMA.svm_reg_test (select id, ind from MADLIB_SCHEMA.svm_train_data limit 20);

select MADLIB_SCHEMA.svm_predict('MADLIB_SCHEMA.svm_reg_test', 'ind', 'id', 'regs', 'MADLIB_SCHEMA.svm_reg_output1', false, 'MADLIB_SCHEMA.svm_dot');
select * from MADLIB_SCHEMA.svm_reg_output1;

select MADLIB_SCHEMA.svm_predict('MADLIB_SCHEMA.svm_reg_test', 'ind', 'id', 'regp', 'MADLIB_SCHEMA.svm_reg_output2', true, 'MADLIB_SCHEMA.svm_dot');
select * from MADLIB_SCHEMA.svm_reg_output2;

-- Example usage for classification:
select MADLIB_SCHEMA.svm_generate_cls_data('MADLIB_SCHEMA.svm_train_data', 10000, 4);
select * from MADLIB_SCHEMA.svm_classification('MADLIB_SCHEMA.svm_train_data', 'clss', false, 'MADLIB_SCHEMA.svm_dot');
select * from MADLIB_SCHEMA.svm_predict('clss', '{10,-20,5,5}', 'MADLIB_SCHEMA.svm_dot') > 0;
select * from MADLIB_SCHEMA.svm_predict('clss', '{-10,20,5,5}', 'MADLIB_SCHEMA.svm_dot') < 0;


-- To learn multiple support vector models, replace the above by 
select * from MADLIB_SCHEMA.svm_classification('MADLIB_SCHEMA.svm_train_data', 'clsp', true, 'MADLIB_SCHEMA.svm_dot');
select pred.prediction > 0 from MADLIB_SCHEMA.svm_predict_combo('clsp', '{10,-20,5,5}', 'MADLIB_SCHEMA.svm_dot') as pred;
select pred.prediction < 0 from MADLIB_SCHEMA.svm_predict_combo('clsp', '{-10,20,5,5}', 'MADLIB_SCHEMA.svm_dot') as pred;

-- Example usage for novelty detection:
select MADLIB_SCHEMA.svm_generate_nd_data('MADLIB_SCHEMA.svm_train_data', 10000, 4);
select * from MADLIB_SCHEMA.svm_novelty_detection('MADLIB_SCHEMA.svm_train_data', 'nds', false, 'MADLIB_SCHEMA.svm_dot');
select * from MADLIB_SCHEMA.svm_predict('nds', '{10,-5,2,2}', 'MADLIB_SCHEMA.svm_dot') > 0;  
select * from MADLIB_SCHEMA.svm_predict('nds', '{-1,-1,2,2}', 'MADLIB_SCHEMA.svm_dot') < 0;  

